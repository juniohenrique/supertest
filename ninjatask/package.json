{
  "name": "ninjatask-api",
  "version": "1.0.0",
  "description": "Task list API",
  "main": "index.js",
  "scripts": {
    "start": "npm run apidoc && npm run clusters",
    "doc": "npm run apidoc",
    "app": "babel-node app.js",
    "clusters": "babel-node clusters.js",
    "test": "mocha test/**/*.js",
    "apidoc": "apidoc -i routes/ -o public/apidoc",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "apidoc": {
    "name": "Ninja Task API - Documentation",
    "template": {
      "forceLanguage": "en"
    }
  },
  "author": "Junio Henrique",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "consign": "^0.1.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "jwt-simple": "^0.5.6",
    "morgan": "^1.9.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "sequelize": "^5.9.2",
    "sqlite3": "^4.0.9",
    "winston": "^2.4.4",
    "snyk": "^1.685.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  },
  "snyk": true
}
